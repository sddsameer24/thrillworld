<script>
    var d = new Date();
    document.getElementById("demo").innerHTML = d;
</script>
<div class="jumbotron">
    <h4>
        <center>
            <strong>Daily Report</strong>

        </center>
    </h4>

    <ol class="breadcrumb">
        <li>
            <i class="fa fa-dashboard"></i>
            <a href="/admin">Dashboard</a>
        </li>
        <li class="active">
            <i class="fa fa-bar-chart-o"></i> {{adminPageTitle}}
        </li>
    </ol>



    <div class="col-md-12">
        <div class="row">
            <div class="col-sm-6 col-md-4 col-md-offset-4 col-sm-offset-3">
                <div id="success" class="alert alert-success" {{#if noMessage}}hidden{{/if}}>
                    {{successMsg}}
                </div>
                <div id="danger" class="alert alert-danger" {{#if noErrorMsg}}hidden{{/if}}>
                    {{errorMsg}}
                </div>
            </div>
        </div>
        <div class="row">
            <div class="container">

                {{#each booking }}
                <div class="row">
                    <input type='hidden' id='id' name='productId' value='{{ this._id }}'>

                    <div>
                        {{#each this.cart}}
                        <label>Event Name</label>
                        <p> {{this.product_name}}</p>
                        <label>Amount per person</label>
                        <p>{{money this.product_price}}</p>
                        {{/each}}

                    </div>

                </div>
                {{/each}}

            </div>
        </div>

    </div>