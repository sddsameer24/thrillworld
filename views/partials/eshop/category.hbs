
<div class="jumbotron" style="height:722px; z-index:-2">
    	<h3 class="text-center" style="
    padding-top: 120px"><a>Watch out for these Destinations</a></h3>
{{#each category}}
 {{#each this.details}}
<div class="col-md-4 col-sm-4 prop">
    <div data-toggle="modal" data-target="#property1" class="mask">
       
        <div class="flip-card">
            <div class="flip-card-inner">
                <div class="flip-card-front">
                    <img src="{{this.imagePath}}" alt="Avatar" style="width:100%;height:260px;">
                </div>
                <div class="flip-card-back">
                    <h2>{{this.name}}</h2>
                    <p>{{this.description}}</p>
                </div>
            </div>
        </div>
    </div>
    <a type="button" class="btn btn-info btn-lg" href="/category/{{this.name}}?q=">Know More
    </a>
   
</div>
 {{/each}}   
{{/each}}
</div>
