<div id="home" class="header">
	<!--start-header-->
	<div class="header-strip w3l">
		<div class="container">
			<p class="phonenum">
				<span class="glyphicon glyphicon-earphone" aria-hidden="true"></span> &nbsp; CALL - US: &nbsp;(+91) 838 100 4848</p>
			<p class="phonenum two">
				<span class="glyphicon glyphicon-home" aria-hidden="true"></span> &nbsp;info@thrillworld.com</p>
			<div class="cartglip pull-right">
				<a href="/shopping-cart">
					<i class="glyphicon glyphicon-shopping-cart"></i> Cart</i>
					{{#if session.cart.totalQty }}
					<span class="badge btn-success">{{ session.cart.totalQty }}</span>
					{{/if}}</a>
			</div>
			<div class="main-search">
				<spam class="close">
					<a>X</a>
				</spam>
				<form>
					<input type="text" value="Search" onFocus="this.value = '';" onBlur="if (this.value == '') {this.value = 'Search';}" class="text"
					/>
					<input type="submit" value="X">

				</form>
				<div class="close glyphicon glyphicon-cross">

				</div>
			</div>
			<div class="srch">
				<button class="search">search</button>
			</div>


			<div class="clearfix"></div>
		</div>
	</div>
	<div class="header-top w3l">
		<div class="container">
			<div class="logo">
				<a href="index.html">
					<h1>
						<img src="./images/logo.png" class="tlogo" alt="">
					</h1>
					<p class="top-para">Dream up for your journey</p>
				</a>
			</div>

			<div class="main-nav pull-right">
				<span class="menu"></span>
				<div class="top-menu">
					<ul class="nav navbar-nav cl-effect-14">
						<li class="active">

							<a href="/">Home</a>
						</li>
						<li class="dropdown">
							<a href="#" class="active">Events
								<i class="fa fa-angle-down"></i>
							</a>
							<ul role="menu" class="sub-menu">
								<li>
									<a href="/" class="active">Events</a>
								</li>
								<li>
									<a href="/shopping-cart">Cart</a>
								</li>
								{{#if login}}
								<li>
									<a href="/user/logout">Logout</a>
								</li>
								{{else}}
								<li>
									<a href="/user/signin">Login</a>
								</li>
								{{/if}}
							</ul>
						</li>
						<li>
							<a href="/user/orders" class="scroll">Orders</a>
						</li>
						<li>
							<a href="#portfolio" class="scroll">Portfolio</a>
						</li>
						<li>
							<a href="#testimonial" class="scroll">Testimonials</a>
						</li>
						<li>
							<a href="#contact" class="scroll">Contact</a>
						</li>
						<li>
							<div>
								<button action="/listing" class="danger btn-lg">Add your listings</button>
							</div>
						</li>
					</ul>
				</div>
			</div>
			<script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
			<script type="text/javascript">
				jQuery(document).ready(function ($) {
					$(".scroll").click(function (event) {
						event.preventDefault();
						$('html,body').animate({ scrollTop: $(this.hash).offset().top }, 900);
					});
				});
			</script>
			<script type="text/javascript">
				$('.main-search').hide();
				$('.search').click(function () {
					$('.main-search').show();
					$('.main-search text').focus();
				}
				);
				$('.close').click(function () {
					$('.main-search').hide();
				});
			</script>